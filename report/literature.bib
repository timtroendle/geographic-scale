@article{Lehner:2013,
  title = {Global River Hydrography and Network Routing: Baseline Data and New Approaches to Study the World's Large River Systems},
  shorttitle = {Global River Hydrography and Network Routing},
  author = {Lehner, Bernhard and Grill, Günther},
  date = {2013},
  journaltitle = {Hydrological Processes},
  volume = {27},
  pages = {2171--2186},
  issn = {1099-1085},
  doi = {10.1002/hyp.9740},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/hyp.9740},
  urldate = {2019-07-05},
  abstract = {Despite significant recent advancements, global hydrological models and their input databases still show limited capabilities in supporting many spatially detailed research questions and integrated assessments, such as required in freshwater ecology or applied water resources management. In order to address these challenges, the scientific community needs to create improved large-scale datasets and more flexible data structures that enable the integration of information across and within spatial scales; develop new and advanced models that support the assessment of longitudinal and lateral hydrological connectivity; and provide an accessible modeling environment for researchers, decision makers, and practitioners. As a contribution, we here present a new modeling framework that integrates hydrographic baseline data at a global scale (enhanced HydroSHEDS layers and coupled datasets) with new modeling tools, specifically a river network routing model (HydroROUT) that is currently under development. The resulting ‘hydro-spatial fabric’ is designed to provide an avenue for advanced hydro-ecological applications at large scales in a consistent and highly versatile way. Preliminary results from case studies to assess human impacts on water quality and the effects of dams on river fragmentation and downstream flow regulation illustrate the potential of this combined data-and-modeling framework to conduct novel research in the fields of aquatic ecology, biogeochemistry, geo-statistical modeling, or pollution and health risk assessments. The global scale outcomes are at a previously unachieved spatial resolution of 500 m and can thus support local planning and decision making in many of the world's large river basins. Copyright © 2013 John Wiley \& Sons, Ltd.},
  langid = {english},
  number = {15}
}

@thesis{Leimgruber:2018,
  title = {Schweizerisches Speicherwasserkraftpotential infolge der Erhöhungen bestehender Talsperren},
  author = {Leimgruber, Andrin},
  date = {2018},
  abstract = {In a first part, this work examines the potential of Swiss storage hydropower due to dam heightening. For each dam, the feasibility of dam heightening by 5\%, 10\% and 20\% of the existing dam height is investigated. Based on the reviews of the dam heightening options, the additional storage capacity is estimated for several scenarios. With dam heightening at 23 reservoirs, the additional storage capacity is 1058 Mio m3 and the additional winter energy production amounts 3.4 TWh. Another scenario takes into account dam heightening by 20\% of the existing dam height at 30 reservoirs. The additional storage capacity amounts 1530 Mio m3 and the winter energy production can be increased by 4.8 TWh. Due to the Energy Strategy 2050, in the year 2050 the needed additional winter energy production amounts between 3 and 7.7 TWh (Piot, 2014). Dam heightening has the potential to provide a considerably part of the needed additional winter energy production. In the second part of the thesis, dam heightening at the Gries gravity dam is considered in more detail. Dam heightening alternatives, by 5\%, 10\% and 20\% of the existing dam height, are investigated. Since the inflows are not sufficient to fill the extended reservoir each year, different locations for a compensation basin are examined. From an energy-policy point of view aiming at increasing the Swiss electricity production from hydropower in winter, the alternative with the highest additional storage capacity is selected for a more detailed study. Within the development of the preliminary project, the stability of the dam, the spillway, the waterway and the compensation basin are examined in more detail. The project costs are estimated at 48 million francs. Currently dam heightening at the Griessee is not economical but would be interesting if the market situation would change.},
  langid = {german}
}

